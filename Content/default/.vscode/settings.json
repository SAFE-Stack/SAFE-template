{
    "tailwindCSS.includeLanguages": {
        "fsharp": "html"
    },
    "tailwindCSS.experimental.classRegex": [
        "\\.className\\s+\"([^\"]*)\"",
        ["\\.className\\s+\\[([^\\]]*)\\]", "\"([^\"]*)\""]
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.vs": true,
        "**/.idea": true,
        "**/.paket": true,
        "**/node_modules": true,
        "**/paket-files": true,
        "**/bin": true,
        "**/obj": true,
        "**/output": true,
        "**/deploy": true,
    }
}
