{
    "$schema": "http://json.schemastore.org/template",
    "author": "Tomasz Heimowski",
    "classifications": [
        "Web",
        "Saturn",
        "Azure",
        "Fable",
        "Elmish"
    ],
    "name": "SAFE-Stack Web App v5.0.4",
    "tags": {
        "language": "F#",
        "type": "project"
    },
    "identity": "SAFE.Template",
    "shortName": "SAFE",
    "sourceName": "SAFE.App",
    "symbols": {
        "minimal": {
            "type": "parameter",
            "dataType": "bool",
            "defaultValue": "false"
        }
    },
    "sources": [
        {
            "condition": "(!minimal)",
            "source": "./default",
            "exclude": "./minimal"
        },
        {
            "condition": "(minimal)",
            "source": "./minimal",
            "exclude": "./default"
        }
    ],
    "SpecialCustomOperations": {
        "**/paket.*": {
            "operations": [
                {
                    "type": "conditional",
                    "configuration": {
                        "if": [
                            "#if"
                        ],
                        "else": [
                            "#else"
                        ],
                        "elseif": [
                            "#elseif"
                        ],
                        "endif": [
                            "#endif"
                        ],
                        "trim": "true",
                        "wholeLine": "true"
                    }
                }
            ]
        },
        "**/*.fsproj": {
            "operations": [
                {
                    "type": "conditional",
                    "configuration": {
                        "if": [
                            "<!--#if"
                        ],
                        "else": [
                            "<!--#else",
                            "#else-->"
                        ],
                        "endif": [
                            "<!--#endif",
                            "#endif-->"
                        ],
                        "trim": "true",
                        "wholeLine": "true"
                    }
                }
            ]
        },
        "**/*.fs": {
            "operations": [
                {
                    "type": "conditional",
                    "configuration": {
                        "if": [
                            "#if",
                            "//#if",
                            "(*#if"
                        ],
                        "else": [
                            "#else",
                            "//#else",
                            "(*#else",
                            "#else*)"
                        ],
                        "endif": [
                            "#endif",
                            "//#endif",
                            "(*#endif",
                            "#endif*)"
                        ],
                        "trim": "true",
                        "wholeLine": "true"
                    }
                }
            ]
        }
    }
}